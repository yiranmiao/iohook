{
  "name": "iohook",
  "version": "1.0.29",
  "description": "Node.js global keyboard and mouse hook (fork)",
  "author": "Everybody",
  "license": "MIT",
  "homepage": "https://github.com/glitter-ai/iohook",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/glitter-ai/iohook.git"
  },
  "bugs": {
    "url": "https://github.com/glitter-ai/iohook/issues"
  },
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "install": "node install.js",
    "build": "node build.js --upload=false",
    "build:ci": "node build.js --all",
    "build:print": "node -e 'require(\"./helpers\").printManualBuildParams()'",
    "test": "jest",
    "lint:dry": "eslint --ignore-path .lintignore .",
    "lint:fix": "eslint --ignore-path .lintignore --fix . && prettier --ignore-path .lintignore --write .",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "docs:deploy": "npm run docs:build && gh-pages -d docs/.vuepress/dist/",
    "release": "git add . && git commit -am 'version bump' && git tag v$(node -p \"require('./package.json').version\") && git push --tags",
    "postinstall": "sed 's/^#include .nan_scriptorigin\\.h./\\/\\/ #include nan_scriptorigin.h/' ./node_modules/nan/nan.h > ./node_modules/nan/nan.h.new && mv ./node_modules/nan/nan.h.new ./node_modules/nan/nan.h"
  },
  "keywords": [
    "hook",
    "electron",
    "nw.js",
    "listener",
    "mousemove",
    "keypress",
    "keyup",
    "global keypress",
    "shortcut"
  ],
  "lint-staged": {
    "examples/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "docs/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "test/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "nugget": "^2.0.1",
    "pump": "^1.0.3",
    "rc": "^1.2.8",
    "tar-fs": "^1.16.3"
  },
  "devDependencies": {
    "@types/node": "^7.0.62",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-only-warn": "^1.0.2",
    "eslint-plugin-prettier": "^3.4.0",
    "fs-extra": "^9.0.1",
    "gh-pages": "^3.2.0",
    "htmlhint": "^0.15.1",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^11.0.0",
    "minimist": "^1.2.5",
    "nan": "^2.22.0",
    "node-abi": "^3.71.0",
    "node-gyp": "^9.4.0",
    "prebuild": "^11.0.4",
    "prettier": "^2.3.1",
    "robotjs": "^0.6.0",
    "tar": "^6.0.5",
    "vuepress": "^1.7.1"
  },
  "supportedTargets": [
    [
      "electron",
      "33.2.1",
      "130"
    ]
  ],
  "targetsToInstall": [
    "electron-130"
  ],
  "directories": {
    "doc": "docs",
    "example": "examples",
    "test": "test"
  }
}